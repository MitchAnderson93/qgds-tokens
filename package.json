{
  "name": "@qld-gov-au/qgds-tokens",
  "version": "2.0.0",
  "description": "This repository is configured to sync tokens via Figma.",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build:tokens",
    "build": "node build-tokens.mjs",
    "build:package": "tsc --build && copyfiles -u 1 src/android/**/*.tokens.xml src/css/**/*.tokens.css src/js/**/*.tokens.js src/scss/**/*.tokens.scss dist",
    "ci": "npm-run-all build test",
    "test": "jest"
  },
  "dependencies": {
    "@tokens-studio/sd-transforms": "^1.2.0",
    "style-dictionary": "^4.0.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-dynamic-import-vars": "^2.1.2",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.12",
    "@web/dev-server": "^0.4.6",
    "@web/rollup-plugin-html": "^2.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.7",
    "rollup": "^4.18.0",
    "rollup-plugin-import-css": "^3.5.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3",
    "typescript-plugin-css-modules": "^5.1.0"
  },
  "keywords": [
    "queensland government",
    "design system",
    "tokens"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qld-gov-au/qgds-tokens.git"
  },
  "author": "QGDS-Team",
